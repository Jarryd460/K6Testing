{
	"name": "templateproject",
	"version": "1.0.0",
	"description": "A template project structure for writing k6 performance tests",
	"keywords": [
		"K6",
		"performance",
		"testing",
		"typescript"
	],
	"homepage": "https://github.com/Jarryd460/K6Testing#readme",
	"bugs": {
		"url": "https://github.com/Jarryd460/K6Testing/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Jarryd460/K6Testing.git"
	},
	"license": "ISC",
	"author": "Jarryd Deane",
	"type": "module",
	"scripts": {
		"format": "npx prettier --write .",
		"k6": "k6 run ./src/tests/multi-scenario-test.ts",
		"lint": "npx eslint .",
		"lint:fix": "npx eslint . --fix",
		"lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --rules sentences-per-line"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"devDependencies": {
		"@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
		"@eslint/js": "^9.27.0",
		"@types/eslint-plugin-markdown": "^2.0.2",
		"@types/k6": "^1.0.2",
		"@vitest/eslint-plugin": "^1.2.1",
		"eslint": "^9.27.0",
		"eslint-import-resolver-node": "^0.3.9",
		"eslint-import-resolver-typescript": "^4.4.1",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-jsdoc": "^50.6.17",
		"eslint-plugin-jsonc": "^2.20.1",
		"eslint-plugin-markdown": "^5.1.0",
		"eslint-plugin-n": "^17.18.0",
		"eslint-plugin-package-json": "^0.31.0",
		"eslint-plugin-perfectionist": "^4.13.0",
		"eslint-plugin-regexp": "^2.7.0",
		"eslint-plugin-yml": "^1.18.0",
		"globals": "^16.2.0",
		"husky": "^9.1.7",
		"markdownlint": "^0.38.0",
		"markdownlint-cli": "^0.45.0",
		"prettier": "^3.5.3",
		"prettier-plugin-curly": "^0.3.2",
		"prettier-plugin-packagejson": "^2.5.14",
		"pretty-quick": "^4.1.1",
		"sentences-per-line": "^0.3.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.33.0"
	},
	"engines": {
		"node": "20",
		"npm": "10"
	}
}
